{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img alt="Git Logo" style="max-width: 20px; max-height: 20px; border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64," />
    {% endset %}
    {% set text %}
        <span>{{ collector.branch }}</span>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">
            <img alt="Git Logo" style="max-width: 32px; max-height: 32px;" src="data:image/png;base64," />
        </span>
        <strong>Git</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Git</h2>

    <p>
        {% if collector.branch is empty %}
            <em>Could not fetch branch name from HEAD.</em>
        {% else %}
            <label><strong>Branch:</strong></label> {{ collector.branch }}
        {% endif %}
    </p>
{% endblock %}